<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="renderer" class="io.erenkaya.akaldiroglu._01_example.domain.StandartOutputRenderer">
        <property name="provider" ref="provider"></property>
    </bean>
    
    <bean id="provider" class="io.erenkaya.akaldiroglu._01_example.domain.StandartOutputProvider"></bean>


    <bean  id="renderer2" class="io.erenkaya.akaldiroglu._01_example.domain.ErrorOutputRenderer">
        <property name="provider" ref="provider2"></property>
    </bean>

    <bean id="provider2" class="io.erenkaya.akaldiroglu._01_example.domain.ErrorOutputProvider"></bean>

</beans>